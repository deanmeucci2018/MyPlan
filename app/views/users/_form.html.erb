<%= form_for(@user) do |f| %>
        	<%= f.label :student_first_name, "First Name" %> 
	    	<%= f.text_field   :student_first_name, class: 'form-control' %> 

        	<%= f.label :student_last_name, "Last Name" %>
	    	<%= f.text_field   :student_last_name, class: 'form-control' %>

        	<%= f.label :email, "Email Address" %>
		<%= f.text_field    :email, class: 'form-control' %>
		
		<%= f.label :grad_year, "Graduation Year" %>
		<!--% f.text_field :grad_year, class: 'form-control' %-->
	
		<%= f.select(:grad_year, [['2018', 2018], ['2019', 2019], ['2020', 2020], ['2021', 2021], ['2022', 2022]]) %>
		<br>
	    	<%= f.label :password, "Password" %>
		<%= f.password_field :password, class: 'form-control' %>

	    	<%= f.label   :password_confirmation, "Confirmation" %>
		<%= f.password_field   :password_confirmation, class: 'form-control' %>
    
		
		<%= f.submit   "Create my account", class: "btn btn-primary" %>
<% end %>