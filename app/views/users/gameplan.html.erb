<h1 style = "text-align: center">GAMEPLAN</h1>

  <nav >
      <h5>Your Interests</h5>
      <ul id ="enrollmentnav">
    <% @userreqs.each do |int| %>
     <% if (int) != @previoustop %>
     
        <li>
          <%= link_to int.interest_name, int%>
        </li> 
         <% @previoustop = (int) %>
       
     <% end %>
    <% end %>
   </ul>
  </nav>




<div class ="row">
    <h5>Sections to Look at for Spring 2018 Semester</h5>
    <br>
    <table style ="width:100%" class="blueTable">
        <thead>
            <tr>
                <th>Dep/number</th>
                <th>Course</th>
                <th>Credits</th>
                <th>Professor</th>
                <th>Semester</th>
                <th>Time</th>
                <th>View</th>
            </tr>
        </thead>
        <% @final.each do |en| %>
                    <tr>
                        <td><%= en.course.department.dep_short_name + '' + en.course.course_number.to_s + '' + en.section_letter%></td>
                        <td><%= en.course.course_full_name.titleize %></td>
                        <td><%= en.course.course_credits %></td>
                        <td><%= en.professor %></td>
                        <td><%= en.semester + ' ' + en.section_year.to_s %></td>
                        <td><%= en.start_time.strftime "%I:%M" %> - <%= en.end_time.strftime "%I:%M %p" %> <%= en.section_days %></td>
                        <td><%= link_to 'Show', en.course %></td>
                    </tr>
        <% end %>
    </table>
</div>

