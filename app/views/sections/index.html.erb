
<h1>Sections</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Professor</th>
      <th>Section letter</th>
      <th>Semester</th>
      <th>Section year</th>
      <th>Section days</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sections.each do |section| %>
      <tr>
        <td><%= section.start_time.strftime "%I:%M %p" %></td>
        <td><%= section.end_time.strftime "%I:%M %p" %></td>
        <td><%= section.professor %></td>
        <td><%= section.section_letter %></td>
        <td><%= section.semester %></td>
        <td><%= section.section_year %></td>
        <td><%= section.section_days %></td>
        <td><%= section.course.course_full_name %></td>
        <td><%= link_to 'Show', section %></td>
        <td><%= link_to 'Edit', edit_section_path(section) %></td>
        <td><%= link_to 'Destroy', section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section', new_section_path %>
